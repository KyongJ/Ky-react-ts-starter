{
  "name": "Ky-react-ts-starter",
  "description": "Quickly create react + typescript project development environment and scaffold for developing npm package components",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:KyongJ/Ky-react-ts-starter.git",
  "author": "Kyong <kyongbest@163.com>",
  "keywords": [
    "react-project",
    "typescript-project",
    "react-typescript",
    "ky-react-ts-stater"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./scripts/config/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config ./scripts/config/webpack.prod.js",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "lint:eslint": "eslint .",
    "lint:stylelint": "stylelint --config .stylelintrc.js src/**/*.{less,css,scss}",
    "lint:fix": "eslint . --fix && stylelint --config .stylelintrc.js 'src/**/*.{css,scss,less}' --fix",
    "prepare": "husky install",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint --config .commitlintrc.js -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --config eslint.config.mjs"
    ],
    "*.{css,less,scss}": [
      "stylelint --config .stylelintrc.js"
    ],
    "*.{ts,tsx,js,json,html,yml,css,less,scss,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.25.9",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@commitlint/cli": "^19.2.0",
    "@commitlint/config-conventional": "^19.1.0",
    "@eslint/js": "^9.17.0",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.3",
    "autoprefixer": "^10.4.18",
    "babel-loader": "^9.2.1",
    "clean-webpack-plugin": "^4.0.0",
    "conventional-changelog-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "less": "^4.2.1",
    "less-loader": "^12.2.0",
    "lint-staged": "^15.3.0",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.4.35",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^8.1.1",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^9.5.1",
    "prettier": "^3.4.2",
    "style-loader": "^3.3.4",
    "stylelint": "^16.2.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-less": "^3.0.1",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "stylelint-order": "^6.0.4",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.0",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.0",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.26.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "browserslist": [
    "defaults",
    "not ie < 11",
    "last 2 versions",
    "> 1%",
    "iOS 7",
    "last 3 iOS versions"
  ]
}
